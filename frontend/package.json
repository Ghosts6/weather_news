{
  "name": "frontend",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"npm:watch:css\" \"npm:watch:js\" \"node --loader ts-node/esm dev.ts\"",
    "watch:css": "tailwindcss -i ./src/input.css -o ./public/css/output.css --watch",
    "watch:js": "esbuild src/index.tsx --bundle --outfile=dist/main.js --loader:.tsx=tsx --loader:.png=dataurl --watch",
    "build:css": "tailwindcss -i ./src/input.css -o ./public/css/output.css",
    "build": "esbuild src/index.tsx --bundle --outfile=dist/main.js --loader:.tsx=tsx --loader:.png=dataurl",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/leaflet": "^1.9.12",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "esbuild": "^0.25.12",
    "express": "^4.19.2",
    "http-proxy-middleware": "^3.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.9.5",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss-textshadow": "^2.1.3"
  }
}
